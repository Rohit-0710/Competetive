package main;

public class HighestNumber {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int[] arr = {1, 5 ,3 ,2, 9 };
		
		int largest = arr[0];
		
		for(int i=0; i<arr.length; i++)
		{
			for(int k =0; k<arr.length; k++)
			{
				if(arr[i] > arr[k])
				{
					largest = arr[i];
				}
			}
		}

		System.out.println("Largest Number is "+ largest);
	}

}
